@model IEnumerable<Post>
@{

}

<h1>Posts</h1>
<div>
    <button type="button" onclick="location.href='@Url.Action("Index","Post")'"> Create new post</button>
</div>
<div>
    <ul>
        @foreach(var post in Model)
        {
            <li>
                <a asp-controller="Post" asp-action="PostView" asp-route-id="@post.Id">
                    @post.Title
                </a>
            </li>
        }
    </ul>
</div>